{
	"name": "@aa900031/eslint-config",
	"type": "module",
	"version": "4.0.0",
	"packageManager": "pnpm@10.19.0",
	"description": "ESLint config for @aa900031",
	"author": {
		"name": "zhong666",
		"email": "hi@zhong666.me"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/aa900031/eslint-config"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.mts",
			"default": "./dist/index.mjs"
		}
	},
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.mts",
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"postinstall": "unbuild --stub",
		"dev": "run-s dev:*",
		"dev:ui": "eslint-config-inspector",
		"build": "unbuild",
		"lint": "eslint . --cache --cache-location ./node_modules/.cache/eslint",
		"release": "release-it"
	},
	"peerDependencies": {
		"eslint": ">=8.0.0"
	},
	"dependencies": {
		"@antfu/eslint-config": "catalog:deps",
		"defu": "catalog:deps",
		"eslint-plugin-case-police": "catalog:deps",
		"eslint-plugin-file-progress": "catalog:deps",
		"is-in-ci": "catalog:deps",
		"local-pkg": "catalog:deps",
		"resolve-pkg": "catalog:deps",
		"semver": "catalog:deps"
	},
	"devDependencies": {
		"@aa900031/eslint-config": "workspace:*",
		"@aa900031/release-it-config": "catalog:dev",
		"@eslint/config-inspector": "catalog:dev",
		"@tsconfig/node22": "catalog:types",
		"@types/node": "catalog:types",
		"@types/semver": "catalog:types",
		"eslint": "catalog:peer",
		"npm-run-all2": "catalog:dev",
		"release-it": "catalog:dev",
		"typescript": "catalog:types",
		"unbuild": "catalog:dev"
	}
}
