{
	"name": "@aa900031/eslint-config",
	"type": "module",
	"version": "3.1.0",
	"packageManager": "pnpm@10.7.1",
	"description": "ESLint config for @aa900031",
	"author": {
		"name": "zhong666",
		"email": "aa900031@gmail.com"
	},
	"license": "MIT",
	"exports": {
		".": {
			"types": "./dist/index.d.mts",
			"default": "./dist/index.mjs"
		}
	},
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.mts",
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"dev": "run-s dev:*",
		"dev:build": "unbuild --stub",
		"dev:ui": "eslint-config-inspector",
		"build": "unbuild",
		"lint": "eslint . --cache --cache-location ./node_modules/.cache/eslint",
		"release": "release-it --ci"
	},
	"peerDependencies": {
		"eslint": ">=8.0.0"
	},
	"dependencies": {
		"@antfu/eslint-config": "4.10.1",
		"defu": "^6.1.4",
		"eslint-plugin-case-police": "^1.0.0",
		"eslint-plugin-file-progress": "^3.0.2",
		"is-in-ci": "^1.0.0",
		"local-pkg": "^1.1.1",
		"resolve-pkg": "^2.0.0",
		"semver": "^7.7.1"
	},
	"devDependencies": {
		"@aa900031/eslint-config": "workspace:*",
		"@eslint/config-inspector": "^1.0.2",
		"@release-it/conventional-changelog": "^9.0.4",
		"@tsconfig/node18": "^18.2.4",
		"@types/node": "^18.19.80",
		"@types/semver": "^7.5.8",
		"conventional-changelog-unjs": "^1.0.0",
		"eslint": "^9.22.0",
		"npm-run-all2": "^7.0.2",
		"release-it": "^17.11.0",
		"typescript": "~5.8.2",
		"unbuild": "^3.5.0"
	}
}
