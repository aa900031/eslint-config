{
	"name": "@aa900031/eslint-config",
	"type": "module",
	"version": "3.1.0",
	"packageManager": "pnpm@10.15.1",
	"description": "ESLint config for @aa900031",
	"author": {
		"name": "zhong666",
		"email": "aa900031@gmail.com"
	},
	"license": "MIT",
	"exports": {
		".": {
			"types": "./dist/index.d.mts",
			"default": "./dist/index.mjs"
		}
	},
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.mts",
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"dev": "run-s dev:*",
		"dev:build": "unbuild --stub",
		"dev:ui": "eslint-config-inspector",
		"build": "unbuild",
		"lint": "eslint . --cache --cache-location ./node_modules/.cache/eslint",
		"release": "release-it --ci"
	},
	"peerDependencies": {
		"eslint": ">=8.0.0"
	},
	"dependencies": {
		"@antfu/eslint-config": "4.19.0",
		"defu": "^6.1.4",
		"eslint-plugin-case-police": "^1.0.0",
		"eslint-plugin-file-progress": "^3.0.2",
		"is-in-ci": "^1.0.0",
		"local-pkg": "^1.1.2",
		"resolve-pkg": "^2.0.0",
		"semver": "^7.7.2"
	},
	"devDependencies": {
		"@aa900031/eslint-config": "workspace:*",
		"@aa900031/release-it-config": "^0.1.0",
		"@eslint/config-inspector": "^1.2.0",
		"@tsconfig/node22": "^22.0.2",
		"@types/node": "^22.18.1",
		"@types/semver": "^7.7.0",
		"eslint": "^9.34.0",
		"npm-run-all2": "^7.0.2",
		"release-it": "^19.0.4",
		"typescript": "~5.8.2",
		"unbuild": "^3.6.1"
	}
}
